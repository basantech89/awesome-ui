{
  "name": "@awesome-tools/awesome-ui",
  "version": "0.0.0-semantically-released",
  "license": "MIT",
  "private": false,
  "description": "Awesome UI for Awesome Tools",
  "repository": {
    "type": "git",
    "url": "https://github.com/basantech89/awesome-ui"
  },
  "homepage": "https://github.com/basantech89/awesome-ui#readme",
  "bugs": {
    "url": "https://github.com/basantech89/awesome-ui/issues"
  },
  "author": {
    "name": "Basant Soni",
    "email": "basantech89@gmail.com"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "types": "dist/types/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "cm": "commitizen",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist/types",
    "build:dev": "babel src -d dist --extensions \".js,.jsx,.ts,.tsx\" --ignore \"**/*.test.ts\" --ignore \"**/*.stories.tsx\"",
    "build": "yarn build:types && cross-env BABEL_ENV=production babel src -d dist --extensions \".js,.jsx,.ts,.tsx\" --ignore \"**/*.test.ts\" --ignore \"**/*.stories.tsx\"",
    "watch:types": "tsc --noEmit -w",
    "lint": "eslint --ext js,jsx,ts,tsx ./src",
    "lint:types": "tsc --noEmit",
    "lint:fix": "eslint --ext js,jsx,ts,tsx --fix ./src",
    "storybook": "start-storybook -p 6006 -s storybook-static",
    "build-storybook": "build-storybook",
    "build-storybook-docs": "build-storybook --docs",
    "test": "cross-env CI=true jest",
    "test:report": "cross-env CI=true jest --coverage",
    "chromatic": "npx chromatic --project-token=${CHROMATIC_PROJECT_TOKEN}",
    "semantic-release": "semantic-release",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "@emotion/react": "^11.4.1",
    "lodash": "^4.17.21",
    "polished": "^4.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.4.3"
  },
  "devDependencies": {
    "@awesome-tools/commitizen": "^1.0.0",
    "@awesome-tools/eslint-config": "^1.0.0",
    "@babel/cli": "^7.14.8",
		"@babel/core": "^7.14.3",
		"@babel/preset-env": "^7.14.8",
		"@babel/preset-react": "^7.14.5",
		"@babel/preset-typescript": "^7.15.0",
		"@commitlint/cli": "^16.1.0",
		"@commitlint/config-conventional": "^16.0.0",
		"@fortawesome/fontawesome-svg-core": "^1.2.35",
		"@fortawesome/free-solid-svg-icons": "^5.15.3",
		"@fortawesome/react-fontawesome": "^0.1.14",
		"@storybook/addon-a11y": "^6.4.13",
		"@storybook/addon-actions": "^6.4.13",
		"@storybook/addon-essentials": "^6.4.13",
		"@storybook/addon-links": "^6.4.13",
		"@storybook/node-logger": "^6.4.13",
		"@storybook/react": "^6.4.13",
		"@types/jest": "^26.0.23",
		"@types/lodash": "^4.14.172",
		"@types/react": "^17.0.9",
		"@types/react-dom": "^17.0.6",
		"babel-loader": "^8.2.2",
		"chromatic": "^6.3.4",
		"cross-env": "^7.0.3",
		"csstype": "^3.0.8",
		"eslint-plugin-import": "^2.24.1",
		"eslint-plugin-prettier": "^3.4.0",
		"husky": "^7.0.0",
		"jest": "^27.0.4",
		"semantic-release": "^19.0.2"
	},
  "keywords": [
    "typescript",
    "javascript",
    "react",
    "storybook",
    "awesome",
    "awesome-tools",
    "awesome-ui"
  ]
}
