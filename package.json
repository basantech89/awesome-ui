{
  "name": "@awesome-tools/awesome-ui",
  "version": "0.0.0-semantically-released",
  "license": "MIT",
  "description": "Awesome UI for Awesome Tools",
  "repository": {
    "type": "git",
    "url": "https://github.com/basantech89/awesome-ui"
  },
  "homepage": "https://github.com/basantech89/awesome-ui#readme",
  "bugs": {
    "url": "https://github.com/basantech89/awesome-ui/issues"
  },
  "author": {
    "name": "Basant Soni",
    "email": "basantech89@gmail.com"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "types": "dist/types/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "cm": "commitizen",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist/types",
    "build:dev": "babel src -d dist --extensions \".js,.jsx,.ts,.tsx\" --ignore \"**/*.test.ts\" --ignore \"**/*.stories.tsx\"",
    "build": "yarn build:types && cross-env BABEL_ENV=production babel src -d dist --extensions \".js,.jsx,.ts,.tsx\" --ignore \"**/*.test.ts\" --ignore \"**/*.stories.tsx\"",
    "watch:types": "tsc --noEmit -w",
    "lint": "eslint --ext js,jsx,ts,tsx ./src",
    "lint:types": "tsc --noEmit",
    "lint:fix": "eslint --ext js,jsx,ts,tsx --fix ./src",
    "storybook": "storybook dev -p 6006 -s storybook-static",
    "build-storybook": "storybook build",
    "build-storybook-docs": "storybook build --docs",
    "test": "cross-env CI=true jest --colors",
    "test:report": "cross-env CI=true jest --coverage",
    "chromatic": "npx chromatic --project-token=${CHROMATIC_PROJECT_TOKEN}",
    "semantic-release": "semantic-release",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "@emotion/react": "^11.4.1",
    "lodash": "^4.17.21",
    "polished": "^4.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@awesome-tools/commitizen": "^1.0.0",
    "@awesome-tools/eslint-config": "^1.0.0",
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@compiled/babel-plugin": "^0.19.1",
    "@compiled/react": "^0.13.1",
    "@compiled/webpack-loader": "^0.11.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@linaria/babel-preset": "^4.5.1",
    "@linaria/core": "^4.5.1",
    "@linaria/react": "^4.5.1",
    "@linaria/webpack-loader": "^4.5.1",
    "@storybook/addon-a11y": "^7.0.27",
    "@storybook/addon-actions": "^7.0.27",
    "@storybook/addon-essentials": "^7.0.27",
    "@storybook/addon-interactions": "^7.0.27",
    "@storybook/addon-links": "^7.0.27",
    "@storybook/jest": "^0.1.0",
    "@storybook/node-logger": "^7.0.27",
    "@storybook/react": "^7.0.27",
    "@storybook/react-webpack5": "^7.0.27",
    "@storybook/test-runner": "^0.11.0",
    "@storybook/testing-library": "^0.2.0",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.172",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "astroturf": "^1.2.0",
    "babel-loader": "^8.2.2",
    "chromatic": "^6.3.4",
    "cross-env": "^7.0.3",
    "csstype": "^3.1.1",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.12",
    "expect-type": "^0.15.0",
    "husky": "^7.0.0",
    "semantic-release": "^19.0.2",
    "storybook": "^7.0.27",
    "ts-expect": "^1.3.0",
    "tsafe": "^1.6.4",
    "tsd": "^0.28.1",
    "typescript": "^5.0.4",
    "vitest": "^0.31.1"
  },
  "keywords": [
    "typescript",
    "javascript",
    "react",
    "storybook",
    "awesome",
    "awesome-tools",
    "awesome-ui"
  ],
  "packageManager": "yarn@3.6.0"
}
